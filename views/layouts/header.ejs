<body>
    <nav class="navbar navbar-expand-lg navbar-light nav-style">
        <div class="container header-text">
            <a class="navbar-brand" href="/">ShoppingMarket</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="nav navbar-nav">
                    <li class="nav-link">
                        <a class="nav-link active" href="/products">Products</a>
                    </li>
                    <li class="nav-link">
                        <a class="nav-link active" href="/by/categorys">By Category</a>
                    </li>

                    <% if(typeof page !=="undefined" ){%>

                        <% page.forEach((p)=>{%>

                            <li class="nav-link text-capitalize">
                                <a class="nav-link active" aria-current="page" href="/<%= p.title %>">
                                    <%=p.title%>
                                </a>
                            </li>
                            <% }) %>
                                <% } %>
                </ul>
                <div class="navbar-nav ms-auto">
                    <% if(typeof carts !=="undefined" && carts.length>0 ){ %>
                        <a href="/all/mycart" class="nav-link">My cart(<%=carts.length %>)</a>
                        <% } else{ %>
                            <a href="/all/mycart" class="nav-link">My cart(0)</a>
                            <% } %>

                                <% if(!currentUser) {%>
                                    <a href="/users/login" class="nav-link">Login</a>
                                    <a href="/users/register" class="nav-link">Register</a>
                                    <% }else if(typeof admin !=="undefined" && admin){ %>
                                        <a href="/admin/products" class="nav-link">control of admin</a>
                                        <a href="/users/logout" class="nav-link">Logout</a>
                                        <% }else{%>
                                            <a href="/users/logout" class="nav-link">Logout</a>
                                            <% } %>
                </div>
            </div>
        </div>
    </nav>
    <% if(success[0]||error[0]){ %>
        <main class="container mt-3">
            <%- include("./flash"); -%>
        </main>
        <% } %>








            <% if(typeof home !=="undefined" ){ %>
                <% if(home.googleId) {%>
                    <img src="<%=u.logo%>">
                    <% } else{ %>
                        <img src="/<%=u.logo%>">
                        <% } %>
                            <% } %>